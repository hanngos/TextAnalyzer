<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.company.controller.FXStartController">
    <Button id="runButton" fx:id="runButton" layoutX="202.0" layoutY="215.0" mnemonicParsing="false"
            onAction="#runButtonClick" prefHeight="25.0" prefWidth="96.0" text="RUN">
        <font>
            <Font size="19.0"/>
        </font>
    </Button>
    <Text layoutX="124.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type in text to be analyzed:">
        <font>
            <Font size="18.0"/>
        </font>
    </Text>
    <TextField id="inputText" fx:id="inputText" layoutX="124.0" layoutY="150.0" prefHeight="25.0" prefWidth="253.0"/>
    <Button layoutX="443.0" layoutY="14.0" mnemonicParsing="false" onAction="#infoButtonClick" text="Info"/>
    <Label layoutX="114.0" layoutY="27.0" text="TEXT ANALYZER PROGRAM">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
</AnchorPane>
